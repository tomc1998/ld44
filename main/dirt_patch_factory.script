local dirt_patch_repo = require "main.dirt_patch_repo"
local render = require "main.render"

function init(self)
   for x = 0,4 do
      for y = 0,4 do
         local p = factory.create("#factory", vmath.vector3(render.get_width()/2 - 32 + x * 16, render.get_height()/2 + y * 16 - 32, 0.0))
         dirt_patch_repo:add_patch(p, x, y)
      end
   end
end
